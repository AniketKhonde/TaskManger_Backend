{
    "version": 2,
    "builds": [
      {
        "src": "src/index.ts",
        "use": "@vercel/node",
        "config": {
          "outputDirectory": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/tasks/(.*)",
        "dest": "/dist/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/index.js"
      }
    ]
  }
  